    skills: [
      "Website Development",
      "Python Web Apps",
      "MERN Stack",
      "API Development",
      "Responsive UI"
    ]
  },
  {
    name: "Dilip Singh",
    role: "Senior Security Analyst | SecOps",
    tag: "Core Security Team",
    desc: "Dilip Singh is a Senior Security Analyst with 12+ years of extensive experience in cybersecurity operations, network security, vulnerability management, and enterprise IT security. He has led SOC operations, SIEM monitoring, endpoint security implementations, and incident response across large-scale enterprise environments.",
    skills: [
      "SOC & SecOps Operations",
      "SIEM Monitoring",
      "Endpoint & Cloud Security",
      "Vulnerability Management",
