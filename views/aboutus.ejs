    ]
  },
  {
    name: "V. Singh",
    role: "Senior Technology Advisor | Enterprise Systems & Security",
    tag: "Advisors & Senior Experts",
    desc: "V. Singh is a senior technology advisor with 25+ years of experience in enterprise IT systems, security strategy, and product engineering. He specializes in enterprise architecture design, advanced security strategy, AI-enabled systems, data engineering, and leading high-impact enterprise consulting engagements.",
    skills: [
      "Enterprise IT Architecture",
      "Security Strategy",
      "Data Engineering",
      "AI-enabled Systems",
      "Product Engineering"
    ]
  }
];

let index = 0;

function render() {
  const m = team[index];
  document.getElementById("name").innerText = m.name;
  document.getElementById("role").innerText = m.role;
  document.getElementById("tag").innerText = m.tag;
  document.getElementById("desc").innerText = m.desc;

  const skills = document.getElementById("skills");
  skills.innerHTML = "";
  m.skills.forEach(skill => {
    skills.innerHTML += `
      <div class="px-4 py-2 rounded-xl bg-purple-600/10 text-purple-300 text-sm text-center">
        ${skill}
      </div>
    `;
  });
}

function nextMember() {
  index = (index + 1) % team.length;
  render();
}

function prevMember() {
  index = (index - 1 + team.length) % team.length;
  render();
}

render();
</script>

